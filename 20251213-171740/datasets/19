create unlogged table t (a bigint, b text) with (fillfactor = 60);
insert into t select -1 * a, b from (select r, a, b, generate_series(0,16-1) AS p from (select row_number() over () AS r, a, b from (select i AS a, md5(i::text) AS b from generate_series(1, 625000) s(i) ORDER BY (i + 1024 * (random() - 0.5))) foo) bar) baz ORDER BY ((r * 16 + p) + 2 * (random() - 0.5));
create index idx on t(a ASC) with (deduplicate_items=false);
